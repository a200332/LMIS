        <header value="[Header]"/>
        <footer value="[Footer]"/>
        -->
				<conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
			</layout>
		</appender>
		<!-- Example of how to configure the LogConsoleAppender	-->
