			<!--<maximumFileSize value="10KB" />-->
			<datePattern value="'_'yyyy.MM.dd-HH'.log'" />
			<staticLogFileName value="true" />
			<encoding value="utf-8" />
			<immediateFlush value="true" />
			<layout type="log4net.Layout.PatternLayout">
